# Docker Compose Environment Variables
# Copy this file to .env in the root directory and fill in your values

# JWT Secrets
JWT_SECRET=your-secret-key-change-this-in-production-min-32-characters-recommended
JWT_REFRESH_SECRET=your-refresh-secret-key-change-this-in-production-min-32-characters

# OAuth
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret

# TossPayments
TOSS_SECRET_KEY=your-toss-secret-key

# Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-specific-password

# Client URL (for CORS)
CLIENT_URL=http://localhost
